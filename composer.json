{
    "name": "laminas/laminas-{component}",
    "description": "Laminas cache adapter",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "cache"
    ],
    "support": {
        "docs": "https://docs.laminas.dev/laminas-{component}/",
        "issues": "https://github.com/laminas/laminas-{component}/issues",
        "source": "https://github.com/laminas/laminas-{component}",
        "rss": "https://github.com/laminas/laminas-{component}/releases.atom",
        "forum": "https://discourse.laminas.dev/"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "laminas/laminas-cache": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27 || ^6.5.8 || ^7.1.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "provide": {
        "laminas/laminas-cache-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Cache\\Adapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Cache\\Adapter\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
